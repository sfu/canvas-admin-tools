{
  "extends": ["unobtrusive"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "warn"
  },
  "ignorePatterns": ["src/http/**/node_modules"]
}
